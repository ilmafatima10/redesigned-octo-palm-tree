#include <iostream>

int main() {
    beg = 0,end=0,mid=0;
    cin>>beg;
    cin>>end;
    cin>>mid;
end = size - 1;  
while ( beg <= end)  
{  
 
mid = (beg + end) / 2;  

if (arr[mid] == num)  
{  
return mid + 1;  
}  
else if (arr[mid] > num)  
{  
    end = mid - 1;  
}  
else if (arr [mid] < num)  
{  
    beg = mid + 1;  
}  
}  

return -1;   

}
